{
    "version": 10,
    "$schema": "https://cmake.org/cmake/help/latest/_downloads/3e2d73bff478d88a7de0de736ba5e361/schema.json",
    "configurePresets": [
        {
            "name": "cdadance-demo",
            "displayName": "CDADance demo setup (no ros, no python, no tests)",
            "inherits": "relwithdebinfo",
            "cacheVariables": {
                "BUILD_TESTING": "OFF",
                "WITH_ROS_SUPPORT": "OFF", // only needed for AutonomousInteraction state in CDADance
                "PYTHON_BINDING": "OFF",
                "WITH_HRP4": "ON",
                "WITH_XSENS_STREAMING": "OFF"
            }
        }
    ],
    "buildPresets": [
        {
            "name": "cdadance-demo",
            "displayName": "CDADance demo setup (no ros, no python, no tests)",
            "configurePreset": "cdadance-demo",
            "configuration": "RelWithDebInfo",
            "targets": [
                "install"
            ]
        }
    ]
}
