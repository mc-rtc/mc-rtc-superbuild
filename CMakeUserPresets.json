{
    "version": 10,
    "$schema": "https://cmake.org/cmake/help/latest/_downloads/3e2d73bff478d88a7de0de736ba5e361/schema.json",
    "configurePresets": [
        {
            "name": "panda_prosthesis-notests",
            "description": "",
            "displayName": "RelWithDebInfo PandaProsthesis (jammy, no tests)",
            "inherits": [
                "relwithdebinfo-notests"
            ],
            "cacheVariables": {
                "WITH_Panda": "ON",
                "WITH_PandaLIRMM": "ON",
                "WITH_UR10": "ON",
                "WITH_MC_RTDE": "OFF",
                "WITH_UR_MODERN_DRIVER": "OFF",
                "WITH_UR_RTDE": "OFF",
                "WITH_PANDA_BRACE": "ON"
            }
        },
        {
            "name": "panda_prosthesis",
            "description": "",
            "displayName": "RelWithDebInfo PandaProsthesis (tests)",
            "inherits": [
                "panda_prosthesis-notests"
            ],
            "cacheVariables": {
                "BUILD_TESTING": "ON"
            }
        }
    ],
    "buildPresets": [
        {
            "name": "panda_prosthesis-notests",
            "displayName": "RelWithDebInfo PandaProsthesis (no tests)",
            "configurePreset": "panda_prosthesis-notests",
            "configuration": "RelWithDebInfo",
            "targets": [
                "all"
            ]
        },
        {
            "name": "panda_prosthesis",
            "displayName": "RelWithDebInfo PandaProsthesis (tests)",
            "configurePreset": "panda_prosthesis",
            "configuration": "RelWithDebInfo",
            "targets": [
                "all"
            ]
        }
    ]
}
